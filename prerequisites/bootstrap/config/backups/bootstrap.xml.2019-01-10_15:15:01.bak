                    <soft-reset-with-stored-info>no</soft-reset-with-stored-info>
                    <enable>yes</enable>
                  </entry>
                </peer-group>
                <policy>
                  <export>
                    <rules>
                      <entry name="BgpExport">
                        <used-by>
                          <member>Passive</member>
                        </used-by>
                        <action>
                          <allow>
                            <update>
                              <med>10</med>
                            </update>
                          </allow>
                        </action>
                      </entry>
                    </rules>
                  </export>
                </policy>
                <routing-options>
                  <graceful-restart>
                    <enable>yes</enable>
                  </graceful-restart>
                </routing-options>
              </bgp>
            </protocol>
